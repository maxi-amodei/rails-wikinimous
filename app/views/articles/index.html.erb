<div class="banner">
<%= link_to "Wikinimous", articles_path %> <br>
   <%= link_to "New Article", new_article_path %>
</div>
<div class="container">
  <h1>My Articles</h1>
  <div class="article-wraper">
    <% @articles.each do |article|%>
      <div class="article">
        <h1><%= link_to article.title, article_path(article.id) %></h1>
        <p><%= article.content %></p>
      </div> 
    <%end %>
    
  </div>
</div>

